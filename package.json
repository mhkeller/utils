    "ci": "npm run test:coverage && npm run format:check",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:generate && node docs/clean-markdown.js && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "node docs/generate-docs.js",
    "docs:validate": "node docs/validate-markdown.js",
    "docs:clean": "node docs/clean-markdown.js",
    "docs:refresh": "npm run docs:generate && npm run docs:clean && npm run docs:validate"
